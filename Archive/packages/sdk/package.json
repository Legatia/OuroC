{
  "name": "@ouroc/sdk",
  "version": "1.0.0",
  "description": "OuroC Core SDK - Decentralized subscriptions on Solana with ICP timers, X.402 payments, and IPFS transaction records",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "ouroc",
    "solana",
    "subscription",
    "recurring-payments",
    "payments",
    "billing",
    "saas",
    "icp",
    "internet-computer",
    "ipfs",
    "x402",
    "http-native-payments",
    "coinbase-x402",
    "jupiter",
    "stablecoins",
    "typescript",
    "web3",
    "defi",
    "crypto-payments"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@coinbase/x402": "^0.6.6",
    "@coral-xyz/anchor": "^0.29.0",
    "@dfinity/agent": "^0.20.2",
    "@dfinity/candid": "^0.20.2",
    "@dfinity/principal": "^0.20.2",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bn.js": "^5.1.5",
    "@types/bs58": "^4.0.4",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "rollup": "^4.52.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-jest": "^29.4.4",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OuroC/OuroC.git",
    "directory": "packages/sdk"
  },
  "license": "MIT",
  "author": "OuroC Team",
  "homepage": "https://OuroC.com",
  "bugs": {
    "url": "https://github.com/OuroC/OuroC/issues"
  }
}
