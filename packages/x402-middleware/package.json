{
  "name": "@ouroc/x402-middleware",
  "version": "1.0.0",
  "description": "X.402 HTTP-native payments middleware (kept for future reference)",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "x402",
    "http-native-payments",
    "payments",
    "middleware",
    "coinbase-x402",
    "ouroc"
  ],
  "author": "OuroC Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ouroc/ouroc-c.git",
    "directory": "packages/x402-middleware"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./express": {
      "import": "./dist/express.esm.js",
      "require": "./dist/express.js",
      "types": "./dist/express.d.ts"
    },
    "./nextjs": {
      "import": "./dist/nextjs.esm.js",
      "require": "./dist/nextjs.js",
      "types": "./dist/nextjs.d.ts"
    }
  },
  "dependencies": {
    "@ouroc/sdk": "file:../sdk"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "rollup": "^4.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "express": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}